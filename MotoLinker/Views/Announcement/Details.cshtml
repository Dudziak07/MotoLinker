@model MotoLinker.Models.Announcement

<h1>@Model.Title</h1>
<p><strong>Opis:</strong> @Model.Description</p>
<p><strong>Cena:</strong> @Model.Price PLN</p>
<p><strong>Lokalizacja:</strong> @Model.Location</p>
<p><strong>Marka:</strong> @Model.Brand</p>
<p><strong>Model:</strong> @Model.Model</p>
<p><strong>Rok produkcji:</strong> @Model.ProductionYear</p>
<p><strong>Data dodania:</strong> @Model.DateAdded.ToString("yyyy-MM-dd")</p>

<p><strong>Właściciel ID:</strong> @Model.UserId</p>

<a asp-action="List" class="btn btn-secondary">Powrót do listy</a>
